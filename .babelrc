{
  "presets": ["babel-preset-gatsby"],
  "plugins": [
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "css",
          "java",
          "typescript",
          "go",
          "shellsession",
          "json",
          "graphql",
          "jsx"
        ],
        "plugins": ["show-language"],
        "theme": "default",
        "css": true
      }
    ],
    ["babel-plugin-styled-components"]
  ]
}
